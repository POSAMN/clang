int findByValue(LinkedList_t* ll, int value){
    node_t* cur = ll->head;
    if (cur != NULL){
        while (cur->next != NULL && cur->data != value){
            cur = cur->next;
        }
        printf("%d\n", cur->data);
        if (cur->data == value){
            return 1;
        } else {
            return 0;
        }
    }
    return 0;
}
